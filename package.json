{
  "name": "jamstack-static-generated-app",
  "description": "A proof of concept for a static generated app.",
  "version": "0.0.1",
  "author": "tobias <tobias@zucali.at>",
  "license": "MIT",
  "scripts": {
    "build:main:app": "gatsby build",
    "disabled-build:main:app": "mkdir -p public && cp -r static/* public",
    "build:main:lambda": "netlify-lambda build src/server/lambda --config webpack.config.js",
    "build:pre:clean": "yarn run clean",
    "build:pre:lint": "yarn run lint",
    "build:pre:test": "yarn run test",
    "build": "run-p build:pre:* && run-p build:main:*",
    "clean": "run-p clean:*",
    "clean:cache": "rimraf .cache",
    "clean:public": "rimraf public",
    "clean:functions": "rimraf lambda/*",
    "deploy": "yarn run netlify deploy",
    "dev": "yarn run clean && yarn run start",
    "graphql:updateSchemaFile": "babel-node internals/graphql/updateSchema.js --config-file ./babel-node.config.js",
    "lint:eslint:fix": "yarn lint:eslint --fix",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "lint:fix": "yarn lint:eslint:fix .",
    "lint": "yarn lint:eslint .",
    "netlify": "netlify",
    "start:lambda": "netlify-lambda serve src/server/lambda --config webpack.config.js",
    "start:netlifyDev": "netlify dev",
    "start:netlifyDev:command": "gatsby develop",
    "start": "run-p start:*",
    "test": "jest",
    "test:debug": "jest --inspect-brk test --runInBand"
  },
  "dependencies": {
    "apollo-link-schema": "^1.2.3",
    "apollo-server-lambda": "^2.6.4",
    "babel-jest": "^24.8.0",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-preset-gatsby": "^0.2.0",
    "chokidar": "^3.0.1",
    "downshift": "^3.2.10",
    "gatsby": "^2.8.3",
    "gatsby-image": "^2.0.20",
    "gatsby-plugin-create-client-paths": "^2.0.2",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-netlify": "^2.0.17",
    "gatsby-plugin-netlify-cms": "^4.0.1",
    "gatsby-plugin-offline": "^2.1.1",
    "gatsby-plugin-react-helmet": "^3.0.2",
    "gatsby-plugin-sharp": "^2.1.3",
    "gatsby-plugin-sitemap": "^2.1.0",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-remark-copy-linked-files": "^2.0.13",
    "gatsby-remark-images": "^3.0.14",
    "gatsby-remark-relative-images": "^0.2.2",
    "gatsby-source-filesystem": "^2.0.38",
    "gatsby-source-graphql": "^2.0.18",
    "gatsby-transformer-remark": "^2.3.12",
    "gatsby-transformer-sharp": "^2.1.21",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "lodash": "^4.17.11",
    "netlify-cli": "^2.11.20",
    "netlify-cms-app": "^2.9.2-beta.1",
    "netlify-identity-widget": "^1.4.14",
    "normalize.css": "^8.0.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-test-renderer": "^16.8.6",
    "rehype-react": "^3.1.0",
    "styled-components": "^4.3.0",
    "urql": "^1.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-react-hooks": "^1.6.0",
    "netlify-lambda": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "2.6.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
